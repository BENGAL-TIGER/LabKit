#!/bin/sh

# can't install coolprop via envirnoment.yml for some reason
# conda install coolprop

# PyCallCoolProp.jl
julia -e 'import Pkg; Pkg.add(path = "./PyCallCoolProp.jl")'

# install more r packages. these will be compiled on the spot
R --quiet -f r/install.R

# enable jupyter extensions
jupyter nbextension enable livemdpreview/livemdpreview